{
      "rewrites": [
        {
          "source": "/(.*)",
          "destination": "/api/index.php"
        }
      ],
      "functions": {
        "api/index.php": {
          "runtime": "php-8.0"
        }
      },
       "routes": [
          {
            "src": "/(.*)",
            "dest": "/public/$1"
          }
      ],
      "buildCommand": "composer install && npm install && npm run prod",
      "outputDirectory": "public"
    }